  <%= link_to post do %>
    <div class="row">
      <div class="col s3">
      <%= image_tag "placeholder-user.png", height:"100px" %> <br>
      <%= post.user.email %>
      </div>
      <div class="col s9">
        <h4><%= post.title %></h4>
        <%= post.content %>
      </div>
    </div>
  <% end %>